{
  "name": "vitepress-plugin-mermaid",
  "version": "2.0.11",
  "description": "Add mermaid support for Vitepress",
  "types": "./dist/mermaid.d.ts",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emersonbottero/vitepress-plugin-mermaid.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vitepress-plugin-mermaid.umd.js",
  "module": "./dist/vitepress-plugin-mermaid.es.mjs",
  "exports": {
    ".": {
      "import": "./dist/vitepress-plugin-mermaid.es.mjs",
      "require": "./dist/vitepress-plugin-mermaid.umd.js"
    },
    "./Mermaid.vue": "./dist/Mermaid.vue"
  },
  "keywords": [
    "vitepress",
    "mermaid"
  ],
  "author": "Emerson Bottero",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emersonbottero/vitepress-plugin-mermaid/issues"
  },
  "homepage": "https://github.com/emersonbottero/vitepress-plugin-mermaid#readme",
  "peerDependencies": {
    "vitepress": "^0.21.6 || ^1.0.0 || ^1.0.0-alpha",
    "mermaid": "^10.2.4"
  },
  "optionalDependencies": {
    "@mermaid-js/mermaid-mindmap": "^9.3.0"
  },
  "devDependencies": {
    "vite": "^4.3.9",
    "vite-plugin-dts": "^3.0.3",
    "vite-plugin-static-copy": "^0.16.0",
    "vitepress": "^1.0.0-beta.5",
    "mermaid": "^10.2.4"
  }
}
